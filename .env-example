# Telegram Bot Token from @BotFather
BOT_TOKEN=your_bot_token_here

# Telegram API ID from https://my.telegram.org/apps
API_ID=123456

# Telegram API Hash from https://my.telegram.org/apps
API_HASH=your_api_hash_here

# MongoDB connection URI
MONGO_URI=mongodb+srv://username:password@cluster.mongodb.net/?retryWrites=true&w=majority

# Name of the MongoDB database
DATABASE_NAME=FileDrawerBot

# Telegram Channel ID for storing files (must be integer, e.g., -1001234567890)
STORAGE_CHANNEL_ID=-1001234567890

# Comma-separated list of admin user IDs (e.g., 123456789,987654321)
ADMIN_USER_IDS=5190902724
